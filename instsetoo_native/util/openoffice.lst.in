            PROGRESSBARCOLOR 0,0,0
            PROGRESSSIZE 444,8
            PROGRESSPOSITION 35,153
            PROGRESSFRAMECOLOR 102,102,102
            PROGRESSTEXTBASELINE 145
            PROGRESSTEXTCOLOR 255,255,255
